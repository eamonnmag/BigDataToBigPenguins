{
	"manifest_version": 2,
	"name": "Big Bollocks To Big Penguins",
	"version": "1.0",
	"description": "Replaces the text 'big data' with 'big penguins'.",
	"icons": {
    "128": "bigdata-penguins.png"
  },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

